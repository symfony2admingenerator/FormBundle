{
    "name": "symfony2admingenerator/form-bundle",
    "type": "symfony-bundle",
    "description": "Twig extension introducing new blocks to separate form HTML structure from CSS decorating it and JS enhancing its functionalities.",
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Gołębiewski",
            "email": "loostro@gmail.com",
            "role": "Symfony2 bundle author"
        },
        {
            "name": "Symfony community",
            "homepage": "https://github.com/symfony2admingenerator/FormBundle/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "symfony/config": "~5.4|~6.4|~7.4|~8.0",
        "symfony/dependency-injection": "~5.4|~6.4|~7.4|~8.0",
        "symfony/form": "~5.4|~6.4|~7.4|~8.0",
        "symfony/http-kernel": "~5.4|~6.4|~7.4|~8.0",
        "twig/twig": "^3.4.3"
    },
    "suggest": {
        "symfony2admingenerator/generator-bundle": "The missing symfony2 admin generator",
        "symfony2admingenerator/form-extensions-bundle": "Symfony2 form extensions for Admingenerator project (also working standalone!)"
    },
    "autoload": {
        "psr-4": {
            "Admingenerator\\FormBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
